{
  "name": "ollama-webui",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "npm run build && npm run preview",
    "build": "tsc && vite build && esbuild ./proxy/server.cjs --bundle --minify --platform=node --outfile=./dist/server.cjs",
    "preview": "node --env-file ./proxy/.env ./dist/server.cjs"
  },
  "dependencies": {
    "@fastify/http-proxy": "^10.0.2",
    "@fastify/static": "^8.0.3",
    "fastify": "^5.1.0",
    "copy-to-clipboard": "^3.3.3",
    "nanoid": "^5.0.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hotkeys-hook": "^4.6.1",
    "react-icons": "^5.3.0",
    "react-markdown": "^9.0.1",
    "react-textarea-autosize": "^8.5.5",
    "react-toastify": "^10.0.6"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "@tailwindcss/typography": "^0.5.15",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "prettier": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.7.2",
    "vite": "^6.0.0"
  }
