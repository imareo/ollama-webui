{
  "name": "ollama-webui",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "npm run build && npm run preview",
    "build": "tsc && vite build && esbuild ./proxy/server.cjs --bundle --minify --platform=node --outfile=./dist/server.cjs",
    "preview": "node --env-file ./proxy/.env ./dist/server.cjs"
  },
  "dependencies": {
    "@fastify/http-proxy": "^11.0.1",
    "@fastify/static": "^8.0.4",
    "@tailwindcss/postcss": "^4.0.0",
    "@tailwindcss/vite": "^4.0.0",
    "copy-to-clipboard": "^3.3.3",
    "fastify": "^5.2.1",
    "nanoid": "^5.0.9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hotkeys-hook": "^4.6.1",
    "react-icons": "^5.4.0",
    "react-markdown": "^9.0.3",
    "react-textarea-autosize": "^8.5.7",
    "react-toastify": "^11.0.3"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@tailwindcss/typography": "^0.5.16",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.3",
    "vite": "^6.0.11"
  }
